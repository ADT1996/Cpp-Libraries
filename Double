#ifndef ADT_DOUBLEH
#define ADT_DOUBLEH

#include<AbsRealNumeric>

class Double : public AbsRealNumeric<double> {
	public:

		static const double MAXVALUE = 1.7 * 10E+308;
		static const double MINVALUE = 1.7 * 10E-308;

		Double() {
			AbsRealNumeric();
		}

		Double(double value){
			AbsRealNumeric(value);
		}

		static string getClassName() {
			return string("Double");
		}
};
#endif
